#Python Program for Sum of squares of first n natural numbers
def squaresum(n) : 
    sum = 0
    for i in range(1, n+1) : 
        sum = sum + (i * i) 
    return sum 
print(squaresum(4))

#Python Program for cube sum of first n natural numbers
def cube(n) : 
    sum = 0
    for i in range(1, n+1) : 
        sum = sum + (i * i * i) 
    return sum 
print(cube(5))
